{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "reposition": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/reposition",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/assets/css",
                "src/assets/images"
            ],
            "styles": [
              "src/assets/css/homeanimation.less",
              "src/styles.css",

              "src/assets/css/responsive.css",

              "src/assets/css/elementor/globalc12e.css",
              "src/assets/css/elementor/post-137dfe.css",
              "src/assets/css/elementor/post-360bc2.css",
              "src/assets/css/elementor/post-531a64b.css",
              "src/assets/css/elementor/post-5362a6b.css",
              "src/assets/css/elementor/post-5370199.css",
              "src/assets/css/elementor/post-538bc12.css",
              "src/assets/css/elementor/post-5397ad4.css",
              "src/assets/css/elementor/post-541a2ff.css",
              "src/assets/css/elementor/post-54250c5.css",
              "src/assets/css/elementor/post-5438aa4.css",
              "src/assets/css/elementor/post-5449055.css",
              "src/assets/css/elementor/post-5451d05.css",
              "src/assets/css/elementor/post-546e142.css",
              "src/assets/css/elementor/post-547b371.css",
              "src/assets/css/elementor/post-63455ac.css",
              "src/assets/css/elementor/post-7e30c.css",
              "src/assets/css/elementor/post-8583557.css",
              "src/assets/css/elementor/post-86b073.css",
              "src/assets/css/elementor/post-957e545.css",
              "src/assets/css/elementor/frontend.min747d.css",

              "src/assets/css/style2.css",
              "src/assets/css/slide/animate.mind5f7.css",
              "src/assets/css/slide/owl.carousel7d13.css",
              "src/assets/css/slide/sa-owl-themed5f7.css",
              "src/assets/css/styled.css",

              "node_modules/slick-carousel/slick/slick.css",
              "node_modules/slick-carousel/slick/slick-theme.css"
            ],
            "scripts": [

              "node_modules/jquery/dist/jquery.min.js",

              "node_modules/slick-carousel/slick/slick.min.js",
              "src/assets/js/homejquery.js"
            ],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "reposition:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "reposition:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "reposition:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [
              "src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "reposition-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "reposition:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "reposition:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "reposition"
}
